{
  "fullPathTracingEnabled": true,
  "runtimeDetectionEnabled": true,
  "hostMalwareSensorEnabled": true,
  "hostNetworkSensorEnabled": true,
  "kubernetesMode": false,
  "exporters": {
    "stdoutExporter": true,
    "httpExporterConfig": {
      "url": "https://report-ks.eudev3.cyberarmorsoft.com",
      "path": "/cloud/v1/hostAlert",
      "queryParams": [
        {
          "key": "customerGUID",
          "value": "<env>"
        }
      ],
      "method": "POST",
      "headers": [
        {
          "key": "X-API-Key",
          "value": "<env>"
        }
      ]
    }
  }
}
